{
  "name": "@jhbhan/rn-form",
  "version": "1.0.17",
  "description": "A React Native Form UI Component",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Jason Bhan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jhbhan/rn-form.git"
  },
  "keywords": [
    "react-native",
    "form",
    "ui",
    "component"
  ],
  "scripts": {
    "npm-pub": "npm run build && npm version patch && npm publish --access public",
    "build": "npm run build:js && npm run build:types",
    "build:js": "babel src --out-dir dist --extensions .ts,.tsx,.js,.jsx",
    "build:types": "tsc --emitDeclarationOnly",
    "build:watch": "babel src --out-dir dist --extensions .ts,.tsx,.js,.jsx --watch"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-native": ">=0.64.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/react": "~19.0.10",
    "install": "^0.13.0",
    "npm": "^11.5.2",
    "typescript": "~5.8.3"
  }
}
